<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>DeltaJordan</Authors>
    <Company>SplatoonDraftLeague</Company>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.MD</PackageLicenseFile>
    <Description>Airtable integration with SDL's drafting system.</Description>
    <Product>SquidDraftLeague.Airtable</Product>
    <PackageId>SquidDraftLeague.Airtable</PackageId>
    <PackageReleaseNotes>Initial Release.</PackageReleaseNotes>
    <Version>1.0.0</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Airtable" Version="1.0.2" />
    <PackageReference Include="NLog" Version="4.6.6" />
    <PackageReference Include="SquidDraftLeague.Draft" Version="1.0.7151.31524" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE.MD">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
